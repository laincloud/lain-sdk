{
    "web": {
        "cmd": "hello", 
        "port": 80, 
        "volumes": [
            "/data", 
            "/var/lib/mysql"
        ], 
        "env": [
            "ENV_A=enva", 
            "ENV_B=envb"
        ]
    }, 
    "notify": {
        "slack": "#hello"
    }, 
    "test": {
        "script": [
            "go test"
        ]
    }, 
    "release": {
        "dest_base": "ubuntu", 
        "copy": [
            {
                "dest": "/usr/bin/hello", 
                "src": "hello"
            }, 
            {
                "dest": "/entry.sh", 
                "src": "entry.sh"
            }
        ]
    }, 
    "build": {
        "prepare": [
            "echo prepare1", 
            "echo prepare2"
        ], 
        "base": "golang", 
        "script": [
            "echo buildscript1", 
            "echo buildscript2"
        ],
        "build_args": [
            "ARG1=arg1",
            "ARG2=arg2"
        ]
    }
}
